{
  "name": "sheet",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/auth/google": {
      "target": "http://localhost:5000"
    },
    "/api/*": {
      "target": "http://localhost:5000"
    }
  },
  "dependencies": {
    "axios": "^0.17.1",
    "d3-reusable": "git+http://github.com/Jiwoopark0508/project-md.git#dev",
    "lodash": "^4.17.4",
    "mathjs": "^3.20.2",
    "moment": "^2.20.1",
    "numeral": "^2.0.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-handsontable": "^0.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.4.0",
    "redux-saga": "^0.16.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.2",
    "styled-components": "^2.4.0",
    "typeface-roboto": "0.0.50"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "yarn npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "yarn run build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2"
  }
}
